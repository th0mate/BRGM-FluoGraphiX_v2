{
  "name": "fluographix-v2",
  "version": "1.8.9",
  "main": "main.js",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "start": "electron .",
    "compose": "electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "th0mate",
        "repo": "BRGM-FluoGraphiX_v2"
      }
    ]
  },
  "dependencies": {
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
